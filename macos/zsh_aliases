alias add-alias="nvim ~/.dotfiles/macos/zsh_aliases"
alias vim="nvim"
alias untar="tar xvf"
alias uz="unzip"
alias yz="yazi"
alias tf="terraform"
alias ap-key="cat ~/.ssh/id_ed25519.pub"
alias tfp="tf plan"
alias tfa="tf apply"
alias tfd="tf destroy"
alias ga="git add ."
alias gc='git commit -m'
alias gp="git push"
alias T="tmux attach -t Work || tmux new -s Work"
alias t-help="~/.dotfiles/sh/./tmux-help"
alias ccom="sudo minicom -b $1 -D /dev/tty$2 "
