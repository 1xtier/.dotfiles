#/bin/bash
function helpcli() {
	 echo "* install nackerNvim"

}
function install() {
	cd ~/.dotfiles
	git submodule init 
	git submodule update
	ln -s $HOME/.dotfiles/nackernvim $HOME/.config/nvim
}

while [ -n "$1" ]
	do
    case "$1" in
    install )start;;
    help )helpcli;;
    esac
	shift
done
