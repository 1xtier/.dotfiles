#!/usr/bin/env bash
  echo "Pages help TMUX:"
  printf "%-20s %s\n" "Perfix" "Command"
  echo "------------------------------"
echo -e "\e[31m Ctrl-b ‚Äì prefix\e[0m"
echo -e "\e[32m1 –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –æ–∫–Ω–∞–º–∏\e[0m (windows)ü™ü:"
cat << EOF
    - prefix+c ‚Äì —Å–æ–∑–¥–∞—Ç—å –Ω–æ–≤–æ–µ –æ–∫–Ω–æ (–Ω–∞ —Å—Ç–∞—Ç—É—Å –ø–∞–Ω–µ–ª–∏ –≤—ã–≥–ª—è–¥–∏—Ç, –∫–∞–∫ –≤–∫–ª–∞–¥–∫–∞);
    - prefix+w ‚Äì –ø–æ–∫–∞–∑–∞—Ç—å —Å–ø–∏—Å–æ–∫ –æ–∫–æ–Ω;
    - prefix+n ‚Äì –ø–µ—Ä–µ–∫–ª—é—á–∏—Ç—å—Å—è –Ω–∞ —Å–ª–µ–¥—É—é—â–µ–µ –æ–∫–Ω–æ;
    - prefix+p ‚Äì –ø–µ—Ä–µ–∫–ª—é—á–∏—Ç—å—Å—è –Ω–∞ –ø—Ä–µ–¥—ã–¥—É—â–µ–µ –æ–∫–Ω–æ;
    - prefix+<–Ω–æ–º–µ—Ä> ‚Äì –ø–µ—Ä–µ–π—Ç–∏ –∫ –æ–∫–Ω—É —Å —É–∫–∞–∑–∞–Ω–Ω—ã–º –Ω–æ–º–µ—Ä–æ–º;
    - prefix+, ‚Äì –ø–µ—Ä–µ–∏–º–µ–Ω–æ–≤–∞—Ç—å —Ç–µ–∫—É—â–µ–µ –æ–∫–Ω–æ.
EOF
echo -e "\e[32m2 –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –ø–∞–Ω–µ–ª—è–º–∏\e[0m (panes)üéõ:"
cat << EOF
    - prefix+% ‚Äì —Ä–∞–∑–¥–µ–ª–∏—Ç—å –æ–∫–Ω–æ –Ω–∞ –ø–∞–Ω–µ–ª–∏ –≤–µ—Ä—Ç–∏–∫–∞–ª—å–Ω–æ;
    - prefix+" ‚Äì —Ä–∞–∑–¥–µ–ª–∏—Ç—å –æ–∫–Ω–æ –Ω–∞ –ø–∞–Ω–µ–ª–∏ –≥–æ—Ä–∏–∑–æ–Ω—Ç–∞–ª—å–Ω–æ;
    - prefix+o ‚Äì –ø–µ—Ä–µ–∫–ª—é—á–∏—Ç—å—Å—è –Ω–∞ —Å–ª–µ–¥—É—é—â—É—é –ø–∞–Ω–µ–ª—å;
    - prefix+q ‚Äì –ø–æ–∫–∞–∑–∞—Ç—å –Ω–æ–º–µ—Ä–∞ –ø–∞–Ω–µ–ª–µ–π (–ø–æ–ª–µ–∑–Ω–æ –¥–ª—è –≤—ã–±–æ—Ä–∞);
    - prefix+x ‚Äì –∑–∞–∫—Ä—ã—Ç—å —Ç–µ–∫—É—â—É—é –ø–∞–Ω–µ–ª—å;
    - prefix+z ‚Äì —Ä–∞–∑–≤–µ—Ä–Ω—É—Ç—å —Ç–µ–∫—É—â—É—é –ø–∞–Ω–µ–ª—å –Ω–∞ –≤–µ—Å—å —ç–∫—Ä–∞–Ω (–∏ –≤–µ—Ä–Ω—É—Ç—å –µ—ë –æ–±—Ä–∞—Ç–Ω–æ);
    - prefix+{ ‚Äì –ø–µ—Ä–µ–º–µ—Å—Ç–∏—Ç—å –ø–∞–Ω–µ–ª—å –≤–ª–µ–≤–æ;
    - prefix+} ‚Äì –ø–µ—Ä–µ–º–µ—Å—Ç–∏—Ç—å –ø–∞–Ω–µ–ª—å –≤–ø—Ä–∞–≤–æ;
    - prefix+! ‚Äì –≤—ã–Ω–µ—Å—Ç–∏ —Ç–µ–∫—É—â—É—é –ø–∞–Ω–µ–ª—å –≤ –æ—Ç–¥–µ–ª—å–Ω–æ–µ –æ–∫–Ω–æ.
EOF
echo -e "\e[32m3 –£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ —Å–µ—Å—Å–∏—è–º–∏\e[0müìö:"
cat << EOF
    - tmux new -s <–∏–º—è> ‚Äì —Å–æ–∑–¥–∞—Ç—å –Ω–æ–≤—É—é —Å–µ—Å—Å–∏—é —Å –∏–º–µ–Ω–µ–º;
    - tmux ls ‚Äì –ø–æ–∫–∞–∑–∞—Ç—å —Å–ø–∏—Å–æ–∫ —Å–µ—Å—Å–∏–π;
    - tmux attach -t <–∏–º—è> ‚Äì –ø–æ–¥–∫–ª—é—á–∏—Ç—å—Å—è –∫ —Å–µ—Å—Å–∏–∏;
    - prefix+d ‚Äì –æ—Ç—Å–æ–µ–¥–∏–Ω–∏—Ç—å—Å—è –æ—Ç —Ç–µ–∫—É—â–µ–π —Å–µ—Å—Å–∏–∏.
EOF
echo -e "\e[32m4 –ù–∞–≤–∏–≥–∞—Ü–∏—è –º–µ–∂–¥—É –ø–∞–Ω–µ–ª—è–º–∏\e[0m (–≤ —Å—Ç–∏–ª–µ vi)üéõ:"
cat << EOF
    - prefix+h ‚Äì –ø–µ—Ä–µ–∫–ª—é—á–∏—Ç—å—Å—è –Ω–∞ –ø–∞–Ω–µ–ª—å —Å–ª–µ–≤–∞;
    - prefix+j‚Äì –ø–µ—Ä–µ–∫–ª—é—á–∏—Ç—å—Å—è –Ω–∞ –ø–∞–Ω–µ–ª—å —Å–Ω–∏–∑—É;
    - prefix+k ‚Äì –ø–µ—Ä–µ–∫–ª—é—á–∏—Ç—å—Å—è –Ω–∞ –ø–∞–Ω–µ–ª—å —Å–≤–µ—Ä—Ö—É;
    - prefix+l ‚Äì –ø–µ—Ä–µ–∫–ª—é—á–∏—Ç—å—Å—è –Ω–∞ –ø–∞–Ω–µ–ª—å —Å–ø—Ä–∞–≤–∞.
EOF
